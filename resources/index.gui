<svg class="layout1" fill="white">
  <image href="images/back.png" />

  <g id="informationDisplay">
    <text class="HiddenOrVisible" id="timeLabel" visibility = "visible"/>
    <text class="HiddenOrVisible" id="dateLabel" visibility = "visible"/>
    <image class="HiddenOrVisible" href="images/sundry/footPrintCropped.png" id="footPrint" visibility = "hidden" />
    <text class="HiddenOrVisible" id="stepsLabel" visibility = "hidden"/>
    <image class="HiddenOrVisible" href="images/sundry/heartCropped.png" id="heart" visibility = "hidden" />
