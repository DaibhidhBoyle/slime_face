    <image href="images/slimes/4thSlime.png" id="jump6" class="jumpAnimation" visibility = "hidden"/>
    <image href="images/slimes/6thSlime.png" id="jump7" class="jumpAnimation" visibility = "hidden"/>
    <image href="images/slimes/7thSlime.png" id="jump8" class="jumpAnimation" visibility = "hidden"/>
    <image href="images/slimes/8thSlime.png" id="jump9" class="jumpAnimation" visibility = "hidden"/>
  </g>

  <image href="images/slimes/1stMainSlime.png" id="slime" pointer-events="visible" visibility =  "visible"/>

  <text id="batteryLabel"/>
</svg>
